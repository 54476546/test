package com.hisense.hitv.resource.domain;

import java.util.HashMap;

/**
 * check节点对象
 * @author zhoudi
 * @version 1.0
 */
public class LogXmlCheckItemVO {

    private String checkType = null;
    //key:id property;value: element name
    private HashMap<String, String> checkPropertyMap = null;
    /**
     * @return the checkType
     */
    public String getCheckType() {
        return checkType;
    }
    /**
     * @param checkType the checkType to set
     */
    public void setCheckType(String checkType) {
        this.checkType = checkType;
    }
    /**
     * @return the checkPropertyMap
     */
    public HashMap<String, String> getCheckPropertyMap() {
        return checkPropertyMap;
    }
    /**
     * @param checkPropertyMap the checkPropertyMap to set
     */
    public void setCheckPropertyMap(HashMap<String, String> checkPropertyMap) {
        this.checkPropertyMap = checkPropertyMap;
    }
  
}
