<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE sqlMapConfig PUBLIC "-//iBATIS.com//DTD SQL Map Config 2.0//EN"
        "http://ibatis.apache.org/dtd/sql-map-config-2.dtd">

<sqlMapConfig>

    <settings
            cacheModelsEnabled="true"
            enhancementEnabled="true"
            maxSessions="64"
            maxTransactions="30"
            maxRequests="128"/>
 	<!--log file -->
	<sqlMap resource="com/hisense/hitv/log/logfile/persistence/sqldao/sqlmapdao/LogFile.xml"/>
	<!-- log xml -->
    <sqlMap resource="com/hisense/hitv/log/persistence/sqlmapdao/LogParseSTVSql.xml"/>
    <sqlMap resource="com/hisense/hitv/log/persistence/sqlmapdao/LogParseSTV40Sql.xml"/>
    <sqlMap resource="com/hisense/hitv/log/persistence/sqlmapdao/LogParseSTV42Sql.xml"/>
    <sqlMap resource="com/hisense/hitv/log/persistence/sqlmapdao/LogParseExceptionSql.xml"/>
</sqlMapConfig>
